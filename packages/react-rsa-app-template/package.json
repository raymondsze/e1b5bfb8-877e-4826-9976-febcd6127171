{
  "name": "react-rsa-app-template",
  "version": "0.0.1",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "start": "node ../react-rsa-scripts/bin/react-rsa-scripts start",
    "translate": "node ../react-rsa-scripts/bin/react-rsa-scripts translate",
    "start:server": "node ../react-rsa-scripts/bin/react-rsa-scripts start:server",
    "build": "node ../react-rsa-scripts/bin/react-rsa-scripts build",
    "build:dll": "node ../react-rsa-scripts/bin/react-rsa-scripts build:dll",
    "build:server": "node ../react-rsa-scripts/bin/react-rsa-scripts build:server",
    "lint": "eslint src/**/*.{js,jsx} && stylelint src/**/*.{js,jsx}",
    "test": "node ../react-rsa-scripts/bin/react-rsa-scripts test"
  },
  "languages": [
    "en",
    "zh-Hans",
    "zh-Hant"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "dependencies": {
    "apollo-client": "^1.2.2",
    "babel-types": "^6.25.0",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "flexboxgrid": "^6.3.1",
    "graphql": "^0.10.1",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "node-emoji": "^1.5.1",
    "ora": "^1.2.0",
    "prop-types": "^15.5.8",
    "react-rsa": "^0.0.1",
    "react": "^15.5.4",
    "react-apollo": "^1.2.0",
    "react-async-bootstrapper": "^1.1.1",
    "react-async-component": "^1.0.0-beta.3",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-form": "^6.6.3",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.0",
    "sanitize.css": "^5.0.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.3",
    "source-map-support": "^0.4.15",
    "styled-components": "2.0.0-17"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-react-rsa-app": "0.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-rsa-app": "0.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-redux-saga": "^0.3.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "prettier": "^1.4.4",
    "prettier-eslint": "^6.3.0",
    "prettier-eslint-cli": "^4.1.0",
    "stylelint": "^7.11.0",
    "stylelint-config-react-rsa-app": "^0.0.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.2"
  }
}
